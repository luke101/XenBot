<Window x:Class="XenBot.CreatePasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XenBot"
        mc:Ignorable="d"
        Title="CreatePasswordWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Password" HorizontalAlignment="Left" Margin="169,175,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="Password" HorizontalAlignment="Left" Margin="239,179,0,0" VerticalAlignment="Top" Width="257"/>
        <Label Content="Confirm" HorizontalAlignment="Left" Margin="169,228,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="Confirm" HorizontalAlignment="Left" Margin="239,232,0,0" VerticalAlignment="Top" Width="257"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="456,295,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
