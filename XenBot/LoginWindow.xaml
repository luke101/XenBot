<Window x:Class="XenBot.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XenBot"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Password" HorizontalAlignment="Left" Margin="161,181,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="Password" HorizontalAlignment="Center" Margin="0,185,0,0" VerticalAlignment="Top" Width="349"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="535,240,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.141,0.527" Click="Button_Click_1"/>
    </Grid>
</Window>
